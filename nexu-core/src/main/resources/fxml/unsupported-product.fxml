<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="unsupported-product" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lu.nowina.nexu.view.ui.UnsupportedProductController">
    <stylesheets>
        <URL value="@/styles/nexu.css"/>
    </stylesheets>

	<center>
		<VBox BorderPane.alignment="CENTER">
			<children>
				<Label styleClass="message" wrapText="true" text="NexU don't know your Signature Product yet. We cannot autoconfigure the connection for you. You can request more support for your product, or try to connect it yourself with more advanced features."/>
			</children>
		</VBox>
	</center>

	<bottom>
		<HBox styleClass="btn-container">
			<children>
				<Button fx:id="requestSupport" wrapText="true" styleClass="btn-primary" mnemonicParsing="false" text="Request support" />
				<Button fx:id="hicSuntDracones" wrapText="true" styleClass="btn-danger" mnemonicParsing="false" text="I know what I'm doing" />
			</children>
		</HBox>
	</bottom>

</BorderPane>
